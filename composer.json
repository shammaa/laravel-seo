{
    "name": "shammaa/laravel-seo",
    "description": "Professional SEO package for Laravel with support for OpenGraph, Twitter Cards, LinkedIn, and Schema.org structured data",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Shadi Shammaa",
            "email": "shadi.shammaa@gmail.com"
        }
    ],
    "require": {
        "php": "^8.1",
        "illuminate/support": "^9.0|^10.0|^11.0",
        "illuminate/http": "^9.0|^10.0|^11.0",
        "illuminate/view": "^9.0|^10.0|^11.0",
        "illuminate/cache": "^9.0|^10.0|^11.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.0",
        "orchestra/testbench": "^8.0|^9.0"
    },
    "autoload": {
        "psr-4": {
            "Shammaa\\LaravelSEO\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Shammaa\\LaravelSEO\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Shammaa\\LaravelSEO\\LaravelSEOServiceProvider"
            ],
            "aliases": {
                "SEO": "Shammaa\\LaravelSEO\\Facades\\SEO"
            }
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}

